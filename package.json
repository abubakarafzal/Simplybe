  "devDependencies": {
    "concurrently": "^8.2.2",
    "cypress": "^13.14.1",
    "cypress-mochawesome-reporter": "^3.8.2",
    "cypress-parallel": "^0.14.0",
    "prettier": "^3.3.1",
    "typescript": "^5.4.5"
  }
}

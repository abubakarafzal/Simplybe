{
  "compilerOptions": {
    "target": "es2017",
    "moduleResolution": "node",
    "baseUrl": ".",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "lib": [
      "es2017",
      "dom"
    ],
    "types": [
      "cypress",
      "node"
    ],
    "paths": {
      "@fixtures/*": [
        "cypress/fixtures/*"
      ]
    }
  },
  "include": [
    "**/*.ts"
  ]
}